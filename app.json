{
  "expo": {
    "name": "MentorMatch",
    "slug": "mentormatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mentormatch.app"
    },
    "android": {
      "package": "com.mentormatch.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "scheme": "mentormatch",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a027433a-a98a-499c-8e19-e4581565b366"
      }
    }
  }
}
